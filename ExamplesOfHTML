<html>    
  <body>  
	
	<script>
		var a=true;
		var b = true + 10;
		var c= false * 100;
		document.write("<br>5>3 is: " + a);
		document.write("<br>True + 10 : " + b); //11
		document.write("<br>False * 100 : " + c); //0

		
	</script>

  </body>
</html>







<html>    
  <body>  
	
	<script>
		var a="100";
		var b = 200;
		var c= a * b;
        var d=true;
		document.write("<br>a: " + a + " And type of a : "+ (typeof a));
		document.write("<br>b: " + b + " And type of b : "+ (typeof b));
        document.write("<br>a * b : " + c + " And type of c : "+ (typeof c));
		document.write("<br>d: " + d + " And type of d : "+ (typeof d));
      	
	</script>

  </body>
</html>






<html>    
  <body>  
	
	<script>
		document.write("<h1>Arithmetic Operators</h1>") ;
        document.write("<br>10+5 : "+ (10+5));	
        document.write("<br>10-5 : "+ (10-5));	
        document.write("<br>10*5 : "+ (10*5));
        document.write("<br>10/5 : "+ (10/5));
        document.write("<br>10 % 6 : "+ (10%6));
        document.write("<h1>Increment Operator ++ Adds 1 to the variable </h1>") ;
        
        document.write("<h2>Pre Increment Operator ++ Adds 1 to the variable; return the var post that </h2>") ;
        var a = 10;
        document.write("<br>Before Incrementing a : "+ a);	
        var b = ++a; //a = a+1; b=a;
        document.write("<br>after pre Increment, a : "+ a + "; b = ++a : " + b);
	
        document.write("<h2>Post Increment Operator ++ Adds 1 to the variable; return the var before incrementing </h2>") ;
        a = 10;
        document.write("<br>Before Incrementing a : "+ a);	
        b = a++; //b=a; a = a+1; 
        document.write("<br>after post Increment, a : "+ a + "; b = a++ : " + b);

        document.write("<h1>Decrement Operator -- Subtracts 1 from the variable </h1>") ;
        
        document.write("<h2>Pre Deccrement Operator -- </h2>") ;
        a = 10;
        document.write("<br>Before Decrementing a : "+ a);	
        var b = --a; //a = a-1; b=a;
        document.write("<br>after pre Decrement, a : "+ a + "; b = --a : " + b);
	
        document.write("<h2>Post Decrement Operator -- </h2>") ;
        a = 10;
        document.write("<br>Before Decrementing a : "+ a);	
        b = a--; //b=a; a = a-1; 
        document.write("<br>after post Decrement, a : "+ a + "; b = a-- : " + b);


	</script>
        <hr>
        <br>
        <br>
  </body>
</html>






<html>    
  <body>  
	
	<script>
		document.write("<h1>Comparision Operators : returns true or false</h1>") ;
        document.write("<br>10>5 : "+ (10>5));	
        document.write("<br>10<5 : "+ (10<5));	
        document.write("<br>10>=5 : "+ (10>=5));	
        document.write("<br>10<=5 : "+ (10<=5));
        document.write("<br>5<=5 : "+ (5<=5));
        document.write("<br>10==5 : "+ (10==5));	
        document.write("<br>5==5 : "+ (5==5));
        document.write("<br>This is true: '5'==5 : "+ ('5'==5));			
        document.write("<br>This is strict equality check : 5===5 : "+ (5===5));
        document.write("<br>This is strict equality check : '5'===5 : "+ ('5'===5));
        document.write("<br>10!=5 : "+ (10!=5));	
        document.write("<br>5!=5 : "+ (5!=5));
        document.write("<br>This is false: '5'!=5 : "+ ('5'!=5));			
        document.write("<br>This is strict inequality check : 5!==5 : "+ (5!==5));
        document.write("<br>This is strict inequality check : '5'!==5 : "+ ('5'!==5));

	</script>
        <hr>
        <br>
        <br>
  </body>
</html>











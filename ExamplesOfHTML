            <input type="radio" name="skill" value="Java">Java
            <input type="radio" name="skill" value="C#">C#
            <br>
            Qualification:
            <input type="checkbox" name="qual" value="Btech">Btech
            <input type="checkbox" name="qual" value="BE">BE
            <input type="checkbox" name="qual" value="Mtech">Mtech
            <input type="checkbox" name="qual" value="MCA">MCA
            <br>
            Address: <textarea rows="10" cols="20"></textarea>
            <br>
            College State: <input list="state" name="collegeState">
            <datalist id="state">
                <option>AP</option>
                <option>Maharastra</option>
                <option>Assam</option>
                <option>Kerala</option>
                <option>UP</option>
            </datalist>
